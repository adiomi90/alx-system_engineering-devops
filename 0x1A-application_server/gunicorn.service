[Unit]
Description=Gunicorn service for AirBnB web application
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=gunicorn --worker 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app --error-logfile /tmp/airbnb-error.log
Restart=always
StandardOutput=append:/tmp/airbnb-access.log
StandardError=append:/temp/aribnb-error.log
SyslogIdentifier=gunicorn

[Install]
WantedBy=multi-user.target               
